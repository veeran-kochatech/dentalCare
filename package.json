    "next-compose-plugins": "^2.2.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-dropzone": "^14.2.3",
    "react-hook-form": "^7.43.3",
    "react-icons": "^4.7.1",
    "react-pdf": "^6.2.2",
    "react-pdf-selection": "^0.6.20",
